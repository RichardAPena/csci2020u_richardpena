<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Client">
   <TextField fx:id="username" layoutX="100.0" layoutY="50.0" />
   <TextField fx:id="message" layoutX="100.0" layoutY="100.0" />
   <Button fx:id="send" layoutX="30.0" layoutY="150.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
   <Button fx:id="exit" layoutX="100.0" layoutY="150.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
   <Label layoutX="30.0" layoutY="50.0" text="Username:" />
   <Label layoutX="30.0" layoutY="100.0" text="Message:" />
</AnchorPane>
